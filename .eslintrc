{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "react/prop-types": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "no-mulitple-empty-lines": 0,
    "no-unused-vars": 0,
    "no-empty": 1
  }
}
